            app:layout_constraintStart_toStartOf="@+id/gett_background"
            app:layout_constraintTop_toTopOf="@+id/gett_background">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/gett_driver_license_edit_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/gray_profile"
                android:drawableEnd="@drawable/ic_download"
                android:focusable="false"
                android:paddingBottom="@dimen/margin_16dp"
                android:textColor="@color/gray_intro_text"
                android:textColorHint="@color/gray_profile"
                android:textSize="@dimen/text_16sp" />

